activation a1 is sigmoid {
	stoopid 5
}

network joe is feedforward {
	layer {
		activation input
		size 2
		biased
	}
	layer {
		activation a1
		size 3
		biased
	}
	layer {
		activation a1
		size 1
	}
}

training {
	type resilient_propagation
	error 0.01%
	epochs 100000
	input csvfile "scripts/xor-4-1.csv"
	output csvfile "scripts/xor-4-2.csv"
}

testing {
	input csvfile "scripts/xor-4-1.csv"
	output memory "xor-3"
}